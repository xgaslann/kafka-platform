{
  "id": null,
  "uid": "kafka-broker",
  "title": "Kafka Broker",
  "tags": [
    "kafka",
    "broker"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Partition Count",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(kafka_server_replicamanager_partitioncount)",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 2,
      "title": "Topic Count",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(count by (topic) (kafka_server_brokertopicmetrics_messagesin_total))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 3,
      "title": "Under Replicated Partitions",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(kafka_server_replicamanager_underreplicatedpartitions)",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Controller",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(kafka_controller_kafkacontroller_activecontrollercount)",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 5,
      "title": "Bytes In Per Sec",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesin_total[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      }
    },
    {
      "id": 6,
      "title": "Bytes Out Per Sec",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesout_total[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      }
    },
    {
      "id": 7,
      "title": "Request Per Sec",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum(rate(kafka_network_requestmetrics_requests_total[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "id": 8,
      "title": "CPU Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\", instance=~\"10.0.101.166.*|10.0.102.239.*|10.0.103.53.*\"}[5m])) * 100)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        }
      }
    },
    {
      "id": 9,
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes{instance=~\"10.0.101.166.*|10.0.102.239.*|10.0.103.53.*\"} / node_memory_MemTotal_bytes{instance=~\"10.0.101.166.*|10.0.102.239.*|10.0.103.53.*\"})) * 100",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        }
      }
    },
    {
      "id": 10,
      "title": "Disk Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "(1 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\", instance=~\"10.0.101.166.*|10.0.102.239.*|10.0.103.53.*\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\", instance=~\"10.0.101.166.*|10.0.102.239.*|10.0.103.53.*\"})) * 100",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        }
      }
    }
  ]
}